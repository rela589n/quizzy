{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "caouecs/laravel-lang": "~4.0",
        "davejamesmiller/laravel-breadcrumbs": "^5.3",
        "digital-creative/collapsible-resource-manager": "^1.2",
        "doctrine/dbal": "^2.10",
        "eminiarts/nova-tabs": "^1.3",
        "epartment/nova-dependency-container": "^1.2",
        "fideloper/proxy": "^4.0",
        "fourstacks/nova-repeatable-fields": "^1.3",
        "froala/nova-froala-field": "^3.3",
        "fzaninotto/faker": "^1.9",
        "laravel/framework": "^8.0",
        "laravel/nova": "^3.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "onnov/detect-encoding": "^1.2",
        "phpoffice/phpword": "^0.17.0",
        "rela589n/nova-date-time-filter": "*",
        "spatie/laravel-permission": "^3.8",
        "staudenmeir/eloquent-has-many-deep": "^1.11",
        "techouse/select-auto-complete": "^1.3",
        "vyuldashev/nova-column-filter": "^1.0",
        "yassi/nova-nested-form": "^3.0",
        "ziffmedia/nova-select-plus": "^1.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "barryvdh/laravel-debugbar": "^3.5",
        "barryvdh/laravel-ide-helper": "^2.8",
        "facade/ignition": "^2.3.6",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    },
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        },
        {
            "type": "path",
            "url": "./nova-components/NovaDateTimeFilter"
        }
    ]
}
